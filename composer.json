{
    "name": "rhosocial/rho.social",
    "description": "rho.social",
    "keywords": ["yii2", "website", "advanced", "rho", "social"],
    "homepage": "https://rho.social/",
    "type": "project",
    "license": "AGPL",
    "support": {
        "issues": "https://github.com/rhosocial/rho.social/issues?state=open",
        "source": "https://github.com/rhosocial/rho.social"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-mongodb": "^2.0",
        "yiisoft/yii2-redis": "dev-master",
        "kartik-v/yii2-widgets": "^3.4",
        "bower-asset/holderjs": "^2.7",
        "bower-asset/font-awesome": "^4.3",
        "bower-asset/query-object": "^2.0",
        "bower-asset/open-sans": "^1.1",
        "bower-asset/morrisjs": "^0.5.1",
        "bower-asset/fastclick": "^1.0",
        "bower-asset/slimScroll": "^1.3",
        "bower-asset/bootstrap-datepicker": "^1.4",
        "bower-asset/moment": "^2.10",
        "bower-asset/sparklines": "^1.0",
        "bower-asset/iCheck": "^1.0",
        "bower-asset/fullcalendar": "^2.3",
        "bower-asset/wysihtml5": "^0.3.0",
        "bower-asset/ionicons": "^2.0",
        "bower-asset/bower-jvectormap": "^1.2",
        "bower-asset/chartjs": "^1.0",
        "bower-asset/jquery-knob": "^1.2",
        "bower-asset/pace": "^1.0",
        "yiisoft/yii2-jui": "^2.0",
        "bower-asset/ckeditor": "*",
        "vistart/yii2-widgets": "*",
        "vistart/yii2-helpers": "dev-master",
        "vistart/yii2-models": "dev-master",
        "phpunit/phpunit": "*",
        "bower-asset/admin-lte": "*",
        "bower-asset/intercooler-js": "*",
        "bower-asset/jquery.nicescroll": "*",
        "vistart/yii2-components": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "common/runtime": "0777",
                    "contact.rho.social/runtime": "0777",
                    "express.rho.social/runtime": "0777",
                    "message.rho.social/runtime": "0777",
                    "my.rho.social/runtime": "0777",
                    "reg.rho.social/runtime": "0777",
                    "rho.social/runtime": "0777",
                    "sso.rho.social/runtime": "0777",
                    "contact.rho.social/web/assets": "0777",
                    "express.rho.social/web/assets": "0777",
                    "message.rho.social/web/assets": "0777",
                    "my.rho.social/web/assets": "0777",
                    "reg.rho.social/web/assets": "0777",
                    "rho.social/web/assets": "0777",
                    "sso.rho.social/web/assets": "0777",
                    "rhosocial": "0755",
                    "rhosocial_test": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "common/config/request/request.php"
            ],
            "generateCookieValidationKey": [
                "contact.rho.social/config/request.php"
            ],
            "generateCookieValidationKey": [
                "express.rho.social/config/request.php"
            ],
            "generateCookieValidationKey": [
                "message.rho.social/config/request.php"
            ],
            "generateCookieValidationKey": [
                "my.rho.social/config/request.php"
            ],
            "generateCookieValidationKey": [
                "reg.rho.social/config/request.php"
            ],
            "generateCookieValidationKey": [
                "rho.social/config/request.php"
            ],
            "generateCookieValidationKey": [
                "sso.rho.social/config/request.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
